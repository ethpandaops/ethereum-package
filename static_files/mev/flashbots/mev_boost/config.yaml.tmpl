# mev-boost configuration with timing games support
# Generated by ethereum-package

# Timeout in milliseconds for get_header requests to relays.
timeout_get_header_ms: 950

# Threshold in milliseconds that marks when in a slot is considered "too late".
late_in_slot_time_ms: 2000

# Relay Configurations
relays:
{{- range $relay := .Relays }}
  - url: {{ $relay.URL }}
    enable_timing_games: {{ $relay.EnableTimingGames }}
    target_first_request_ms: {{ $relay.TargetFirstRequestMs }}
    frequency_get_header_ms: {{ $relay.FrequencyGetHeaderMs }}
{{- end }}
